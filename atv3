int quantidadePães, quantidadeBroas;
decimal Tpães, TBroas, totalarrecadado, valorguardar;
Console.WriteLine("Digite a quantidade de pães vendidos ");
quantidadePães = Convert.ToInt32(Console.ReadLine());

Console.WriteLine("Digite a quantidade de Broas vendiadas ");
quantidadeBroas = Convert.ToInt32(Console.ReadLine());

Tpães = quantidadePães * 0.12m;

TBroas = quantidadeBroas * 1.50m;

totalarrecadado = Tpães + TBroas;

valorguardar = totalarrecadado * 0.10m;

Console.WriteLine("Total arrecadado : " + totalarrecadado);
Console.WriteLine("Valor a ser guardado na poupança (10%): R$" + valorguardar);
